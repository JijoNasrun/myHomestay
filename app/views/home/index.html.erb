<h1>Home#index</h1>
<p>New user? <%= link_to "Sign out", logout_path%></p>
<p>List of HomeStay</p>


<table>
	<tr><td>ID</td>
	<td>Price</td></tr>

	<% @homestays.each do |homestay| %>
	<tr>
	<td><%= homestay.id %></td>
	<td><%= homestay.price %></td>		
	<td><%= link_to('Book Now!!', booking_new_path(:id => homestay.id)) %></td>
	</tr>
	<% end %>
</table>
